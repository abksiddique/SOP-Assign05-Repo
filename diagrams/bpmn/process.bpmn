  <bpmn:process id="Process_LiteratureReview" isExecutable="true">
    <bpmn:laneSet id="LaneSet_1">
      <bpmn:lane id="Lane_Researcher" name="Researcher">
        <bpmn:flowNodeRef>StartEvent_1</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_IdentifyResearchFocus</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_ExtractKeywords</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_FormulateSearchQuery</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_OpenGoogleScholar</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_ExecuteSearch</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_ApplyDateFilter</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_VerifyResultCount</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_ScanFirstPage</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_MorePapersToProcess</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_IsPaperRelevant</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_CheckAccessType</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_OpenPaperPage</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_AccessUCLogin</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_DownloadPDF</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_SavePDF</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>EndEvent_1</bpmn:flowNodeRef>
      </bpmn:lane>
    </bpmn:laneSet>
    <bpmn:startEvent id="StartEvent_1" name="Start Literature Review">
      <bpmn:outgoing>Flow_StartToIdentify</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:task id="Task_IdentifyResearchFocus" name="Identify Research Focus">
      <bpmn:incoming>Flow_StartToIdentify</bpmn:incoming>
      <bpmn:outgoing>Flow_IdentifyToExtract</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="Task_ExtractKeywords" name="Extract Primary Keywords">
      <bpmn:incoming>Flow_IdentifyToExtract</bpmn:incoming>
      <bpmn:outgoing>Flow_ExtractToFormulate</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="Task_FormulateSearchQuery" name="Formulate Search Query String">
      <bpmn:incoming>Flow_ExtractToFormulate</bpmn:incoming>
      <bpmn:incoming>Flow_ResultTooNarrowBroad</bpmn:incoming>
      <bpmn:outgoing>Flow_FormulateToOpenGS</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="Task_OpenGoogleScholar" name="Open Google Scholar">
      <bpmn:incoming>Flow_FormulateToOpenGS</bpmn:incoming>
      <bpmn:outgoing>Flow_OpenGSToExecute</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="Task_ExecuteSearch" name="Type Query &amp; Execute Search">
      <bpmn:incoming>Flow_OpenGSToExecute</bpmn:incoming>
      <bpmn:outgoing>Flow_ExecuteToFilter</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="Task_ApplyDateFilter" name="Apply Date Filter &#39;Since 2020&#39;">
      <bpmn:incoming>Flow_ExecuteToFilter</bpmn:incoming>
      <bpmn:outgoing>Flow_FilterToGatewayResult</bpmn:outgoing>
    </bpmn:task>
    <bpmn:exclusiveGateway id="Gateway_VerifyResultCount" name="Result Count?">
      <bpmn:incoming>Flow_FilterToGatewayResult</bpmn:incoming>
      <bpmn:outgoing>Flow_ResultTooNarrowBroad</bpmn:outgoing>
      <bpmn:outgoing>Flow_ResultAcceptable</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:task id="Task_ScanFirstPage" name="Scan First Page for Relevant Papers">
      <bpmn:incoming>Flow_ResultAcceptable</bpmn:incoming>
      <bpmn:incoming>Flow_MorePapersYes</bpmn:incoming>
      <bpmn:outgoing>Flow_ScanToMorePapersGateway</bpmn:outgoing>
    </bpmn:task>
    <bpmn:exclusiveGateway id="Gateway_MorePapersToProcess" name="More Papers to Process?">
      <bpmn:incoming>Flow_ScanToMorePapersGateway</bpmn:incoming>
      <bpmn:incoming>Flow_PaperNotRelevant</bpmn:incoming>
      <bpmn:incoming>Flow_NotAvailable</bpmn:incoming>
      <bpmn:incoming>Flow_SaveToMorePapers</bpmn:incoming>
      <bpmn:outgoing>Flow_MorePapersYes</bpmn:outgoing>
      <bpmn:outgoing>Flow_MorePapersNo</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:exclusiveGateway id="Gateway_IsPaperRelevant" name="Is Current Paper Relevant?">
      <bpmn:incoming>Flow_MorePapersYes</bpmn:incoming>
      <bpmn:outgoing>Flow_PaperRelevant</bpmn:outgoing>
      <bpmn:outgoing>Flow_PaperNotRelevant</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:exclusiveGateway id="Gateway_CheckAccessType" name="Check Access Type?">
      <bpmn:incoming>Flow_PaperRelevant</bpmn:incoming>
      <bpmn:outgoing>Flow_OpenAccess</bpmn:outgoing>
      <bpmn:outgoing>Flow_Paywall</bpmn:outgoing>
      <bpmn:outgoing>Flow_NotAvailable</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:task id="Task_OpenPaperPage" name="Open Paper Page (Open Access)">
      <bpmn:incoming>Flow_OpenAccess</bpmn:incoming>
      <bpmn:outgoing>Flow_OpenPageToDownload</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="Task_AccessUCLogin" name="Access UC Institutional Login">
      <bpmn:incoming>Flow_Paywall</bpmn:incoming>
      <bpmn:outgoing>Flow_LoginToDownload</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="Task_DownloadPDF" name="Download PDF File">
      <bpmn:incoming>Flow_OpenPageToDownload</bpmn:incoming>
      <bpmn:incoming>Flow_LoginToDownload</bpmn:incoming>
      <bpmn:outgoing>Flow_DownloadToSave</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="Task_SavePDF" name="Save PDF with Naming Convention">
      <bpmn:incoming>Flow_DownloadToSave</bpmn:incoming>
      <bpmn:outgoing>Flow_SaveToMorePapers</bpmn:outgoing>
    </bpmn:task>
    <bpmn:endEvent id="EndEvent_1" name="End Literature Review">
      <bpmn:incoming>Flow_MorePapersNo</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="Flow_StartToIdentify" sourceRef="StartEvent_1" targetRef="Task_IdentifyResearchFocus" />
    <bpmn:sequenceFlow id="Flow_IdentifyToExtract" sourceRef="Task_IdentifyResearchFocus" targetRef="Task_ExtractKeywords" />
    <bpmn:sequenceFlow id="Flow_ExtractToFormulate" sourceRef="Task_ExtractKeywords" targetRef="Task_FormulateSearchQuery" />
    <bpmn:sequenceFlow id="Flow_FormulateToOpenGS" sourceRef="Task_FormulateSearchQuery" targetRef="Task_OpenGoogleScholar" />
    <bpmn:sequenceFlow id="Flow_OpenGSToExecute" sourceRef="Task_OpenGoogleScholar" targetRef="Task_ExecuteSearch" />
    <bpmn:sequenceFlow id="Flow_ExecuteToFilter" sourceRef="Task_ExecuteSearch" targetRef="Task_ApplyDateFilter" />
    <bpmn:sequenceFlow id="Flow_FilterToGatewayResult" sourceRef="Task_ApplyDateFilter" targetRef="Gateway_VerifyResultCount" />
    <bpmn:sequenceFlow id="Flow_ResultTooNarrowBroad" name="&lt;100 or &gt;5000" sourceRef="Gateway_VerifyResultCount" targetRef="Task_FormulateSearchQuery" />
    <bpmn:sequenceFlow id="Flow_ResultAcceptable" name="100-5000" sourceRef="Gateway_VerifyResultCount" targetRef="Task_ScanFirstPage" />
    <bpmn:sequenceFlow id="Flow_ScanToMorePapersGateway" sourceRef="Task_ScanFirstPage" targetRef="Gateway_MorePapersToProcess" />
    <bpmn:sequenceFlow id="Flow_MorePapersYes" name="Yes, pick next paper" sourceRef="Gateway_MorePapersToProcess" targetRef="Gateway_IsPaperRelevant" />
    <bpmn:sequenceFlow id="Flow_MorePapersNo" name="No more papers" sourceRef="Gateway_MorePapersToProcess" targetRef="EndEvent_1" />
    <bpmn:sequenceFlow id="Flow_PaperRelevant" name="Relevant" sourceRef="Gateway_IsPaperRelevant" targetRef="Gateway_CheckAccessType" />
    <bpmn:sequenceFlow id="Flow_PaperNotRelevant" name="Skip current paper" sourceRef="Gateway_IsPaperRelevant" targetRef="Gateway_MorePapersToProcess" />
    <bpmn:sequenceFlow id="Flow_OpenAccess" name="Open Access" sourceRef="Gateway_CheckAccessType" targetRef="Task_OpenPaperPage" />
    <bpmn:sequenceFlow id="Flow_Paywall" name="Paywall" sourceRef="Gateway_CheckAccessType" targetRef="Task_AccessUCLogin" />
    <bpmn:sequenceFlow id="Flow_NotAvailable" name="Not Available" sourceRef="Gateway_CheckAccessType" targetRef="Gateway_MorePapersToProcess" />
    <bpmn:sequenceFlow id="Flow_OpenPageToDownload" sourceRef="Task_OpenPaperPage" targetRef="Task_DownloadPDF" />
    <bpmn:sequenceFlow id="Flow_LoginToDownload" sourceRef="Task_AccessUCLogin" targetRef="Task_DownloadPDF" />
    <bpmn:sequenceFlow id="Flow_DownloadToSave" sourceRef="Task_DownloadPDF" targetRef="Task_SavePDF" />
    <bpmn:sequenceFlow id="Flow_SaveToMorePapers" sourceRef="Task_SavePDF" targetRef="Gateway_MorePapersToProcess" />
  </bpmn:process>
